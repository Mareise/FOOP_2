Object subclass: #ScuResourceManager	instanceVariableNames: 'cache images imageSize'	classVariableNames: ''	poolDictionaries: ''	category: 'Scu007-Resources'!!ScuResourceManager commentStamp: 'Matthias Springer 2/10/2012 14:54:15.216' prior: 0!A ScuResourceManager is a renders and caches images.Instance Variables	cache:		Mapping from item symbol to rendered image form.	imageSize:		Image resolution in pixels.	images:		Mapping from image symbols to images.!!ScuResourceManager methodsFor: 'accessing' stamp: 'Matthias Springer 1/31/2012 22:22:36.868'!cache	cache ifNil: [self cache: IdentityDictionary new].	^ cache! !!ScuResourceManager methodsFor: 'accessing' stamp: 'Matthias Springer 1/31/2012 22:04:16.864'!cache: anObject	cache := anObject.! !!ScuResourceManager methodsFor: 'accessing' stamp: 'Matthias Springer 2/10/2012 14:52:07.98'!imageSize	imageSize ifNil: [self imageSize: 48@48]. 	^ imageSize! !!ScuResourceManager methodsFor: 'accessing' stamp: 'msw 2/9/2012 13:30:31.973'!imageSize: anObject	imageSize := anObject.	self cache: nil. "reset image cache"! !!ScuResourceManager methodsFor: 'accessing' stamp: 'dominik 2/5/2012 19:32:41.696'!images	"translates itemSymbol to accessor for image"	images ifNil: [ self images: (IdentityDictionary newFromPairs: {			#floor0000. [self class svgFloor0000].			#floor0001. [self class svgFloor0001].			#floor0010. [self class svgFloor0010].			#floor0011. [self class svgFloor0011].			#floor0100. [self class svgFloor0100].			#floor0101. [self class svgFloor0101].			#floor0110. [self class svgFloor0110].			#floor0111. [self class svgFloor0111].			#floor1000. [self class svgFloor1000].			#floor1001. [self class svgFloor1001].			#floor1010. [self class svgFloor1010].			#floor1011. [self class svgFloor1011].			#floor1100. [self class svgFloor1100].			#floor1101. [self class svgFloor1101].			#floor1110. [self class svgFloor1110].			#floor1111. [self class svgFloor1111].			#player. [self class svgPlayer ].			#playerwest. [self class svgPlayerLeft ].			#playereast. [self class svgPlayerRight].			#playernorth. [self class svgPlayerUp ].			#playersouth. [self class svgPlayerDown ].			#monster. [self class svgMonster].			#wall. [self class svgWall].			#bucket. [self class svgBucket].			#slime. [self class svgSlime].			#water. [self class svgWater].			#pickUpItem. [self class svgPickup].			#pickUpItemextend. [self class svgPickupExtend].			#pickUpItembucket. [self class svgPickupBucket].			#pickUpItemsoap. [self class svgPickupSoap].			#pickUpItemcake. [self class svgPickupCake].			#pickUpItemfast. [self class svgPickupFast].			#pickUpItemportal. [self class svgPickupPortal].			#pickUpItemheart. [self class svgPickupHeart].			#portalnorth. [self class svgPortalNorth].			#portaleast. [self class svgPortalEast].			#portalsouth. [self class svgPortalSouth].			#portalwest. [self class svgPortalWest].			#pickUpItemhealth. [self class svgHeart].			#score. [self class svgScore].			#default. [self class svgDefault]})].	^ images! !!ScuResourceManager methodsFor: 'accessing' stamp: 'Matthias Springer 1/31/2012 22:22:40.125'!images: anObject	^ images := anObject! !!ScuResourceManager methodsFor: 'render' stamp: 'Matthias Springer 2/9/2012 16:06:04.298'!formFor: aSymbol	| svgString svgMorph form |	^ self cache at: aSymbol ifAbsentPut: [		svgString := (self images at: aSymbol ifAbsent: [self images at: #default]) value.		form := Form extent: self imageSize depth: 32.		svgMorph := self svgFromString: svgString.		svgMorph extent: self imageSize.		svgMorph fullDrawOn: form getCanvas.		form]! !!ScuResourceManager methodsFor: 'render' stamp: 'Matthias Springer 2/9/2012 14:09:05.46'!formFor: aSymbol inVariation: aVariation	aVariation		ifNotNil: [^ self formFor: (aSymbol, aVariation) asSymbol]  		ifNil: [^ self formFor: aSymbol].! !!ScuResourceManager methodsFor: 'render' stamp: 'Matthias Springer 2/9/2012 14:10:32.926'!svgFromString: aString	| doc |	doc := XMLDOMParser parseDocumentFrom: aString readStream.	^ (SVGMorph new createFromSVGDocument: doc)		color: Color transparent;		yourself! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!ScuResourceManager class	instanceVariableNames: ''!!ScuResourceManager class methodsFor: 'svg' stamp: 'dominik 12/22/2011 15:15:15.693'!svgBucket	^ '<?xml version="1.0" encoding="UTF-8" standalone="no"?><!!-- Created with Inkscape (http://www.inkscape.org/) --><svg   xmlns:dc="http://purl.org/dc/elements/1.1/"   xmlns:cc="http://creativecommons.org/ns#"   xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"   xmlns:svg="http://www.w3.org/2000/svg"   xmlns="http://www.w3.org/2000/svg"   xmlns:xlink="http://www.w3.org/1999/xlink"   xmlns:sodipodi="http://sodipodi.sourceforge.net/DTD/sodipodi-0.dtd"   xmlns:inkscape="http://www.inkscape.org/namespaces/inkscape"   width="32px"   height="32px"   id="svg2985"   version="1.1"   inkscape:version="0.48.2 r9819"   sodipodi:docname="bucket.svg">  <defs     id="defs5554">    <linearGradient       id="linearGradient10214">      <stop         style="stop-color:#888a85;stop-opacity:1;"         offset="0"         id="stop10216" />      <stop         style="stop-color:#414141;stop-opacity:1;"         offset="1"         id="stop10218" />    </linearGradient>    <linearGradient       gradientTransform="matrix(0.94113994,0.33801718,-0.33801718,0.94113994,-233.26269,-206.90078)"       y2="116.26823"       x2="561.1015"       y1="263.24826"       x1="561.1015"       gradientUnits="userSpaceOnUse"       id="linearGradient4825"       xlink:href="#linearGradient4475"       inkscape:collect="always" />    <radialGradient       gradientUnits="userSpaceOnUse"       gradientTransform="matrix(1.8096451,0,0,4.9855289,-451.22833,-758.07963)"       r="14.857927"       fy="190.20804"       fx="557.31622"       cy="190.20804"       cx="557.31622"       id="radialG